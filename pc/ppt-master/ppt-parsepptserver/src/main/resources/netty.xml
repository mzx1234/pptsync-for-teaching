<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="pptSyncServer" class="com.mzx.pptparseserver.netty.pptsync.PPTSyncServer">
        <property name="port" value="${pptsync.port}"></property>
    </bean>

    <bean id="fileLoadServer" class="com.mzx.pptparseserver.netty.fileload.FileLoadServer">
        <property name="port" value="${pptload.port}"></property>
    </bean>
</beans>